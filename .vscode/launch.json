    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug MCControlNAOqi",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/src/mc_naoqi",
            "args": ["-h", "192.168.100.184", "-p", "9559", "-f", "~/mc_playground/local/etc/MyFirstController.yaml"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
              {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
              }
            ],
          }
    ]
}